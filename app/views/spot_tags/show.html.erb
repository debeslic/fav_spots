<div class="page-header">
  <h3>Spot Tag #<%= @spot_tag.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Spot</dt>
      <dd><%= @spot_tag.spot_id %></dd>

      <dt>Tag</dt>
      <dd><%= @spot_tag.tag_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/spot_tags" class="btn btn-primary">
        Back
      </a>
      <a href="/spot_tags/<%= @spot_tag.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_spot_tag/<%= @spot_tag.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
